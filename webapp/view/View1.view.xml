<mvc:View
    controllerName="zov.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:core="sap.ui.core"
    xmlns:m="sap.m"
    xmlns:t="sap.ui.table"
    xmlns="sap.m"
>
    <Page
        id="page"
        title="{i18n>title}"
    >
        <content />

        <t:Table
            id="_IDGenTable1"
            rows="{/OV_HeaderSet}"
            class="sapUiSmallMargin"
            selectionMode="MultiToggle"
            paste="onPaste"
            ariaLabelledBy="title"
        >
            <t:extension>
                <m:OverflowToolbar
                    id="_IDGenOverflowToolbar1"
                    style="Clear"
                >
                    <m:Title
                        id="title"
                        text="Ordens"
                    />
                </m:OverflowToolbar>
            </t:extension>

            <t:columns>
                <t:Column
                    id="_IDGenColumn1"
                    sortProperty="OrdemID"
                    filterProperty="OrdemID"
                    autoResizable="true"
                    width="80px"
                >
                    <m:Label
                        id="_IDGenLabel1"
                        text="{i18n>_IDGenColumn1}"
                    />
                    <t:template>
                        <m:Text
                            id="_IDGenText1"
                            text="{OrdemID}"
                            wrapping="false"
                        />
                    </t:template>
                </t:Column>

                <t:Column
                    id="_IDGenColumn2"
                    sortProperty="DataCricao"
                    filterProperty="DataCricao"
                    autoResizable="true"
                    width="120px"
                >
                    <m:Label
                        id="_IDGenLabel2"
                        text="{i18n>_IDGenColumn2}"
                    />
                    <t:template>
                        <m:Text
                            id="_IDGenText2"
                            text="{path: 'DataCriacao', type: 'sap.ui.model.odata.type.Date', formatOptions: { UTC:true, pattern: 'dd/mm/yyyy'}}"
                            wrapping="false"
                        />
                    </t:template>
                </t:Column>

                <t:Column
                    id="_IDGenColumn3"
                    sortProperty="CriadoPor"
                    filterProperty="CriadoPor"
                    autoResizable="true"
                    width="100px"
                >
                    <m:Label
                        id="_IDGenLabel3"
                        text="{i18n>_IDGenColumn3}"
                    />
                    <t:template>
                        <m:Text
                            id="_IDGenText3"
                            text="{CriadoPor}"
                            wrapping="false"
                        />
                    </t:template>
                </t:Column>

                <t:Column
                    id="_IDGenColumn4"
                    sortProperty="ClienteID"
                    filterProperty="ClienteID"
                    autoResizable="true"
                    width="80px"
                >
                    <m:Label
                        id="_IDGenLabel4"
                        text="{i18n>_IDGenColumn4}"
                    />
                    <t:template>
                        <m:Text
                            id="_IDGenText4"
                            text="{ClienteID}"
                            wrapping="false"
                        />
                    </t:template>
                </t:Column>

                <t:Column
                    id="_IDGenColumn5"
                    sortProperty="TotalItens"
                    filterProperty="TotalItens"
                    autoResizable="true"
                    width="90px"
                >
                    <m:Label
                        id="_IDGenLabel5"
                        text="{i18n>_IDGenColumn5}"
                    />
                    <t:template>
                        <m:Text
                            id="_IDGenText5"
                            text="{TotalItens}"
                            wrapping="false"
                        />
                    </t:template>
                </t:Column>

                <t:Column
                    id="_IDGenColumn6"
                    sortProperty="TotalFrete"
                    filterProperty="TotalFrete"
                    autoResizable="true"
                    width="90px"
                >
                    <m:Label
                        id="_IDGenLabel6"
                        text="{i18n>_IDGenColumn6}"
                    />
                    <t:template>
                        <m:Text
                            id="_IDGenText6"
                            text="{TotalFrete}"
                            wrapping="false"
                        />
                    </t:template>
                </t:Column>

                <t:Column
                    id="_IDGenColumn7"
                    sortProperty="TotalOrdem"
                    filterProperty="TotalOrdem"
                    autoResizable="true"
                    width="100px"
                >
                    <m:Label
                        id="_IDGenLabel7"
                        text="{i18n>_IDGenColumn7}"
                    />
                    <t:template>
                        <m:Text
                            id="_IDGenText7"
                            text="{TotalOrdem}"
                            wrapping="false"
                        />
                    </t:template>
                </t:Column>

                <t:Column
                    id="_IDGenColumn8"
                    sortProperty="Status"
                    filterProperty="Status"
                    autoResizable="true"
                    width="60px"
                >
                    <m:Label
                        id="_IDGenLabel8"
                        text="{i18n>_IDGenColumn8}"
                    />
                    <t:template>
                        <m:Text
                            id="_IDGenText8"
                            text="{Status}"
                            wrapping="false"
                        />
                    </t:template>
                </t:Column>
            </t:columns>
        </t:Table>

        <!-- <Button id="_IDGenButton1"
            text="Novo Cliente"
            press="onNewCustomer"
            ariaDescribedBy="defaultButtonDescription genericButtonDescription"
        >
            <layoutData>
                <FlexItemData id="_IDGenFlexItemData1" growFactor="1" />
            </layoutData>
        </Button>

        <Button id="_IDGenButton2"
            text="Modificar Cliente"
            press="onEditCustomer"
            ariaDescribedBy="defaultButtonDescription genericButtonDescription"
        >
            <layoutData>
                <FlexItemData id="_IDGenFlexItemData2" growFactor="1" />
            </layoutData>
        </Button> -->
    </Page>
</mvc:View>
